{
  "name": "stlatica-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "api:gen": "cd ../shared/openapi/ && pnpm install && pnpm internal:bundle && cd ../../frontend && pnpm orval",
    "api:mock": "cd ../shared/openapi && pnpm install && pnpm internal:mock",
    "build": "next build",
    "cc": "pnpm ci-check",
    "ci-check": "next lint --fix --max-warnings 0 && pnpm ci:test && pnpm ladle build && pnpm build",
    "ci:ladle": "ladle build",
    "ci:lint": "next lint --max-warnings 0",
    "ci:test": "vitest run --coverage",
    "dev": "next dev -H 0.0.0.0",
    "dev:turbo": "next dev --turbo -H 0.0.0.0",
    "vrt": "cross-env E2E_ENV=dev pnpm playwright test --project=visual-snapshot",
    "vrt:update": "cross-env E2E_ENV=dev pnpm playwright test --project=visual-snapshot -u",
    "ci:vrt": "pnpm playwright test --project=visual-snapshot",
    "ladle:dev": "pnpm ladle dev -h 0.0.0.0",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report playwright/html",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "plop": "plop --plopfile plop/plopfile.js",
    "pn": "pnpm",
    "start": "next start",
    "test": "vitest --ui --api.host=0.0.0.0 --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@mantine/core": "^7.6.2",
    "@mantine/hooks": "^7.6.2",
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.8",
    "next": "14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-easy-crop": "^5.0.5",
    "react-hook-form": "^7.51.1",
    "react-icons": "^4.12.0",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ladle/react": "^2.17.2",
    "@playwright/test": "^1.42.1",
    "@stoplight/prism-cli": "^5.6.0",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@testing-library/react": "^14.2.2",
    "@types/node": "20.4.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/sync-fetch": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "8.46.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.10.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jsdom": "^22.1.0",
    "orval": "^6.25.0",
    "plop": "^3.1.2",
    "postcss": "^8.4.37",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "sync-fetch": "^0.5.2",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^0.34.6"
  },
  "volta": {
    "node": "20.10.0",
    "pnpm": "8.13.1"
  }
}